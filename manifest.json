{
    "name": "Export WhatsApp Polls",
    "version": "0.1.1",
    "description": "Export Your WhatsApp Polls To Excel",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png",
        "512": "icons/512.png"
    },
    "action": {
        "default_icon": "icons/128.png"
    },
    "manifest_version": 3,
    "content_scripts": [
        {
            "js": [
                "script.js"
            ],
            "matches": [
                "https://web.whatsapp.com/"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "*.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}